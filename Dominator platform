        // --- Login Logic ---
        document.getElementById('login-form').addEventListener('submit', function(event) {
            event.preventDefault();
            const emailInput = document.getElementById('email').value;
            const passwordInput = document.getElementById('password').value;
            const validUser = users.find(user => user.email === emailInput && user.password === passwordInput);
            if (validUser) { document.getElementById('login-section').style.display = 'none'; document.getElementById('main-content').style.display = 'block'; }
            else { alert('Ø§Ù„Ø¨Ø±ÙŠØ¯ Ø§Ù„Ø¥Ù„ÙƒØªØ±ÙˆÙ†ÙŠ Ø£Ùˆ ÙƒÙ„Ù…Ø© Ø§Ù„Ù…Ø±ÙˆØ± ØºÙŠØ± ØµØ­ÙŠØ­Ø©!'); }
        });
        
        // --- Populate Email List Modal ---
        const emailListContainer = document.getElementById('email-list-container');
        emailListContainer.innerHTML = users.map(user => `<p><strong>Email:</strong> ${user.email} | <strong>Password:</strong> ${user.password}</p>`).join('');

        // --- Modal Logic ---
        const openModalButtons = document.querySelectorAll('[data-modal-target]');
        const showAccountsBtn = document.getElementById('show-accounts-btn');
        openModalButtons.forEach(button => { button.addEventListener('click', () => { const modal = document.querySelector(button.dataset.modalTarget); openModal(modal); }); });
        showAccountsBtn.addEventListener('click', (e) => { e.preventDefault(); const emailModal = document.getElementById('modal-emails'); openModal(emailModal); });

        function openModal(modal) {
            if (modal == null) return;
            modal.classList.add('active');
            const closeButton = modal.querySelector('.modal-close-btn');
            const closeHandler = () => { closeModal(modal); closeButton.removeEventListener('click', closeHandler); modal.removeEventListener('click', overlayClickHandler); };
            const overlayClickHandler = (e) => { if (e.target === modal) { closeHandler(); } };
            closeButton.addEventListener('click', closeHandle
